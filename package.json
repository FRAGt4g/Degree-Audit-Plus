{
  "name": "degree_audit_plus",
  "description": "An extension to create 4-year plans at UT Austin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "wxt prepare",
    "test": "vitest run",
    "format": "prettier --write ./src/**/*.{svelte,html,ts}",
    "lint": "eslint './src/**/*.{ts,svelte}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,svelte}'",
    "prelint": "npm run format",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.7",
    "@tsconfig/svelte": "^5.0.4",
    "@types/chrome": "^0.0.280",
    "@wxt-dev/module-svelte": "^2.0.0",
    "eslint": "^9.23.0",
    "eslint-plugin-svelte": "^3.5.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.25.6",
    "svelte-check": "^4.0.7",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.29.0",
    "vitest": "^3.1.1",
    "wxt": "^0.19.29"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.15",
    "tailwindcss": "^4.0.15"
  }
}
